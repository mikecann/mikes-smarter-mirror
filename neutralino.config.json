{
  "applicationId": "mikes.smarter.mirror",
  "cli": {
    "binaryName": "mikes-smarter-mirror",
    "binaryVersion": "5.2.0",
    "clientVersion": "5.2.0",
    "distributionPath": "/dist/",
    "extensionsPath": "/extensions/",
    "frontendLibrary": {
      "buildCommand": "bun run build",
      "devCommand": "bun run dev",
      "devUrl": "http://localhost:4373",
      "initCommand": "bun install",
      "patchFile": "/index.html",
      "projectPath": "/"
    },
    "resourcesPath": "/dist/"
  },
  "defaultMode": "chrome",
  "documentRoot": "/dist/",
  "enableNativeAPI": true,
  "enableServer": true,
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "modes": {
    "window": {
      "center": true,
      "enableInspector": true,
      "exitProcessOnClose": false,
      "hidden": false,
      "icon": "/public/icon.png",
      "maximize": false,
      "minHeight": 400,
      "minWidth": 700,
      "resizable": true,
      "title": "Mikes Smarter Mirror",
      "useSavedState": false
    }
  },
  "nativeAllowList": ["app.*", "computer.*", "events.*", "filesystem.*", "os.*", "window.*"],
  "singlePageServe": true,
  "tokenSecurity": "one-time",
  "url": "/",
  "version": "1.0.0"
}
